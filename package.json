{
  "name": "witney",
  "amdName": "witney",
  "version": "0.11.0",
  "description": "Don't wait to start with your javascript and typescript projects",
  "source": "lib/main.ts",
  "main": "dist/main.js",
  "module": "dist/main.m.js",
  "umd:main": "dist/main.umd.js",
  "esmodule": "dist/main.modern.js",
  "exports": {
    ".": {
      "browser": "./dist/main.m.js",
      "umd": "./dist/main.umd.js",
      "import": "./dist/main.m.js",
      "require": "./dist/main.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "types": "dist/main.d.ts",
  "authors": [
    "Katja Lutz <opensource@katjalutz.ch>"
  ],
  "homepage": "https://github.com/witneyjs/witney",
  "repository": {
    "type": "git",
    "url": "https://github.com/witneyjs/witney.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.1",
    "badgen": "^3.2.1",
    "chai": "^4.2.0",
    "fs-extra": "^9.0.1",
    "jest": "^26.4.2",
    "mallery": "1.0.0-alpha.7",
    "microbundle": "^0.12.3",
    "prettier": "^2.1.1",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "lib",
    "dist"
  ],
  "standard-version": {
    "scripts": {
      "postchangelog": "node scripts/badges.js && node scripts/site.js && git add assets site README.md"
    }
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "json-summary"
    ],
    "coveragePathIgnorePatterns": [
      "main.(?:tsx|ts|jsx|js)",
      "/lib/tpl-lib/",
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "lib/**/*.{ts,tsx,js,jsx}"
    ]
  }
}
